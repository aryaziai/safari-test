// use div selector below to see if commit was succesful
// remember to wait 5 minutes to see changes

div {
	background-color: orange !important;
}


// instagram

div['data-testid="mobile-nav-logged-in"'] > div:nth-child(2) {
  display: none !important;
}

a[href="/explore/"], div[data-testid="new-post-button"]  {
  display: none !important;
}


// twitter

a[href="/explore"], div[data-testid="placementTracking"] {
  display: none !important;
}

header[role="banner"] > div:nth-child(1) {
  display: none;
}

header[role="banner"] > div:nth-child(2) {
  overflow: hidden !important;
  position: relative !important;
}